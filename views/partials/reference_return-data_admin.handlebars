<tr>
   <td><a class="text-primary" uk-icon="plus-circle" href="#detail_{{id}}" onclick="imageFetch({{id}})" uk-toggle></a></td>
   <td>{{user.name}}</td>
   <td>{{return_sku}}</td>
   <td>{{return_num}}</a></td>
   <td>{{tracking_num}}</td>
   <td>{{first_name}} {{last_name}}</td>
   <td>{{condition}}</td>
   <td>{{amazon_sku}}</td>
   <td>{{date_added}}</td>
</tr>
 <div id="detail_{{id}}" uk-modal>
   <div class="uk-modal-dialog">
      <button class="uk-modal-close-default" type="button" uk-close></button>
      <form class="p-3">
         <h3 class="text-center">Return SKU: {{#if return_sku}}{{return_sku}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</h3>
         <div class="uk-margin text-center" uk-margin>
                  <div uk-form-custom="target: true">
                     <input type="file" id="detailImages{{id}}" accept="image/*, .pdf" multiple>
                     <input class="uk-input uk-form-width-medium" type="text" placeholder="Select file(s)" disabled>
                  </div>
                  <a class="uk-button uk-button-default" id="uploadBtn{{id}}" onclick="trigger({{id}}, {{user_id}})" uk-icon="cloud-upload"></a>
                  <a class="uk-button uk-button-secondary" id="fake{{id}}" type="button" disabled style="display: none;">
                     <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                     Loading...
                  </a>
         </div>
                <ul class="py-2">
                    <li>Client: {{#if user.name}}{{user.name}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Account: {{#if account.name}}{{account.name}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Return #: {{#if return_num}}{{return_num}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Tracking #: {{#if tracking_num}}{{tracking_num}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Date Added: {{#if date_added}}{{date_added}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <hr>
                    <li>Name: {{#if last_name}}{{last_name}}, {{first_name}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Address: {{#if address}}{{address}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>City: {{#if city}}{{city}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>State: {{#if state}}{{state}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Zip Code: {{#if zipcode}}{{zipcode}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Contact #: {{#if phone}}{{phone}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <hr>
                    <li>Amazon SKU: {{#if amazon_sku}}{{amazon_sku}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Condition: {{#if condition}}{{condition}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Description: {{#if description}}{{description}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <li>Status: {{#if status}}{{status}}{{else}}<span class="text-secondary">Not Available</span>{{/if}}</li>
                    <hr>
                     <li id="image{{id}}" class="p-3"></li>
                </ul>
      </form>
   </div>
</div>
